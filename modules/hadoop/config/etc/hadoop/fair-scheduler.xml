<allocations>
    <userMaxAppsDefault>500</userMaxAppsDefault>
    <queueMaxAppsDefault>500</queueMaxAppsDefault>
    <defaultFairSharePreemptionTimeout>10</defaultFairSharePreemptionTimeout>
    <defaultMinSharePreemptionTimeout>5</defaultMinSharePreemptionTimeout>
    <defaultQueueSchedulingPolicy>drf</defaultQueueSchedulingPolicy>
    <queue name="default">
        <minResources>204800 mb, 50 vcores</minResources>
        <maxResources>1024000 mb, 250 vcores</maxResources>
        <schedulingMode>fair</schedulingMode>
        <weight>0.2</weight>
        <minSharePreemptionTimeout>2</minSharePreemptionTimeout>
    </queue>
    <queue name="kylin">
        <minResources>204800 mb, 50 vcores</minResources>
        <maxResources>1024000 mb, 250 vcores</maxResources>
        <schedulingMode>fair</schedulingMode>
        <weight>0.3</weight>
        <minSharePreemptionTimeout>2</minSharePreemptionTimeout>
    </queue>
    <queue name="long_running">
        <minResources>204800 mb, 50 vcores</minResources>
        <maxResources>2048000 mb, 500 vcores</maxResources>
        <schedulingMode>fair</schedulingMode>
        <weight>0.4</weight>
        <minSharePreemptionTimeout>2</minSharePreemptionTimeout>
    </queue>
    <queue name="ai">
        <minResources>204800 mb, 50 vcores</minResources>
        <maxResources>3072000 mb, 750 vcores</maxResources>
        <schedulingMode>fair</schedulingMode>
        <weight>0.3</weight>
        <minSharePreemptionTimeout>2</minSharePreemptionTimeout>
    </queue>
    <queue name="streaming">
        <minResources>204800 mb, 50 vcores</minResources>
        <maxResources>2048000 mb, 500 vcores</maxResources>
        <schedulingMode>fair</schedulingMode>
        <weight>0.4</weight>
        <minSharePreemptionTimeout>2</minSharePreemptionTimeout>
    </queue>
    <queue name="bi">
        <minResources>204800 mb, 50 vcores</minResources>
        <maxResources>2048000 mb, 500 vcores</maxResources>
        <schedulingMode>fair</schedulingMode>
        <weight>0.3</weight>
        <minSharePreemptionTimeout>2</minSharePreemptionTimeout>
    </queue>
    <queue name="parquet_convert">
        <minResources>204800 mb, 50 vcores</minResources>
        <maxResources>2048000 mb, 500 vcores</maxResources>
        <schedulingMode>fair</schedulingMode>
        <weight>0.5</weight>
        <minSharePreemptionTimeout>2</minSharePreemptionTimeout>
    </queue>
    <queue name="open_resource">
        <minResources>204800 mb, 50 vcores</minResources>
        <maxResources>1024000 mb, 250 vcores</maxResources>
        <schedulingMode>fair</schedulingMode>
        <weight>0.3</weight>
        <minSharePreemptionTimeout>2</minSharePreemptionTimeout>
    </queue>
    <queue name="dw_etl">
        <minResources>204800 mb, 50 vcores</minResources>
        <maxResources>3072000 mb, 750 vcores</maxResources>
        <schedulingMode>fair</schedulingMode>
        <weight>0.5</weight>
        <minSharePreemptionTimeout>2</minSharePreemptionTimeout>
    </queue>
</allocations>
